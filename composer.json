{
    "name": "toporia/audit",
    "description": "Audit logging for Toporia Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Phungtruong7820",
            "email": "minhphung485@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "toporia/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Toporia\\Audit\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
